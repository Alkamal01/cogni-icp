{
  "version": 1,
  "canisters": {
    "cogni-icp-frontend": {
      "type": "assets",
      "source": [
        "src/cogni-icp-frontend/dist"
      ],
      "dependencies": [
        "cogni-icp-backend"
      ]
    },
    "cogni-icp-backend": {
      "candid": "src/cogni-icp-backend/cogni-icp-backend.did",
      "package": "cogni-icp-backend",
      "type": "rust"
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm --prefix src/cogni-icp-frontend run build",
      "args": ""
    }
  },
  "output_env_file": ".env"
}